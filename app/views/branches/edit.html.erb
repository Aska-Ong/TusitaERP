<% provide(:title, 'Edit Branch') %>
<h1>Edit Branch</h1>

<div class="row">
  <div class="span6 offset3">
    <%= simple_form_for (@branch) do |f| %>
  <%= render 'shared/branch_error' %>
      <%= f.input :name %>

      <%= f.submit 'Save changes', class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>