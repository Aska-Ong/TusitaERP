<% provide(:title, 'Edit Transport Record') %>
<h1>Edit Transport Record</h1>

<div class="row">
  <div class="span6 offset3">
    <%= simple_form_for (@transport) do |f| %>
  <%= render 'shared/transport_error' %>
      <%= f.input :transit_id %>

      <%= f.input :name %>

      <%= f.submit 'Save changes', class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>